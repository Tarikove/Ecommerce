@model IEnumerable<Microsoft.AspNetCore.Identity.IdentityRole> 

<h5>Liste des rôles</h5>
<a asp-action="CreateRole" >Créer un role</a><hr />
<p></p>
<table class="table table-striped table-bordered">
    <thead>
        <tr>
            <td>Id</td>
            <td>Role</td>
        </tr>
    </thead>
    <tbody>
        @foreach(var role in Model)
        {
        <tr>
            <td>@role.Id</td>
            <td>@role.Name</td>
        </tr>
        }
    </tbody>

    </table>

